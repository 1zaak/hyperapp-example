{
  "name": "mobcut-frontend",
  "version": "0.0.1",
  "description": "The frontend for Mobcut",
  "main": "index.js",
  "dependencies": {
    "@hyperapp/router": "git://github.com/hyperapp/router.git#newrouter",
    "auth0-js": "^8.12.0",
    "graphql-request": "^1.4.0",
    "history": "^4.7.2",
    "hyperapp": "^0.16.2",
    "lodash": "^4.17.4"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.5",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.22.1",
    "babel-loader": "^7.1.2",
    "babel-minify-webpack-plugin": "^0.2.0",
    "babel-plugin-transform-react-jsx": "^6.22.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-es2015-ie": "^6.7.0",
    "bulma": "^0.6.0",
    "css-loader": "^0.28.7",
    "eslint": "^4.12.1",
    "eslint-config-airbnb-base": "^12.0.0",
    "eslint-plugin-import": "^2.2.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "imports": "^1.0.0",
    "node-libs-browser": "^2.0.0",
    "node-sass": "^4.7.2",
    "postcss-cli": "^4.1.1",
    "sass-loader": "^6.0.6",
    "style-loader": "0.19.0",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.9.7"
  },
  "scripts": {
    "css-deploy": "npm run css-build && npm run css-postcss",
    "css-build": "node-sass ./src/_styles/main.scss ./bin/bundle.css",
    "css-postcss": "postcss --use autoprefixer --output ./bin/bundle.css ./bin/bundle.css",
    "css-watch": "npm run css-build -- --watch",
    "build": "npm run css-deploy | webpack --env production",
    "start": "npm run build && npm run css-watch | webpack-dev-server",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
